<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat code coverage">
				<Configuration>
					<Format>opencover</Format>
					<Exclude>
						[InventoryManagementAPI]InventoryManagementAPI.Migrations.*
						[InventoryManagementAPI]InventoryManagementAPI.Migrations/*
					</Exclude>
					<Include>[InventoryManagementAPI]</Include>
					<ExcludeByFile>**/Migrations/*.*</ExcludeByFile>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
</RunSettings>
